{
  "name": "@knide/fs-prober",
  "version": "0.0.4",
  "description": "fs-prober is a browser-friendly NPM package for extracting file and folder structures from user-selected files or directories.",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "LICENSE",
    "CHANGELOG.md",
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint:tsc": "tsc --noEmit",
    "lint:biome": "biome check ./src",
    "lint": "yarn lint:tsc && yarn lint:biome",
    "format": "biome check --write ./src"
  },
  "dependencies": {
    "lodash.flatmap": "^4.5.0",
    "lodash.map": "^4.6.0",
    "nanoid": "^5.0.9",
    "react-dropzone": "^14.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/lodash.flatmap": "^4",
    "@types/lodash.map": "^4",
    "@types/wicg-file-system-access": "^2023.10.5",
    "lefthook": "^1.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": ">= 16.8 || 18.0.0"
  },
  "author": "ashuvssut (Ashutosh Khanduala)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stacknide/fs-prober/issues"
  },
  "homepage": "https://github.com/stacknide/fs-prober#readme",
  "keywords": [
    "fs-prober",
    "file",
    "system",
    "probe",
    "folder",
    "structure",
    "browser"
  ]
}
